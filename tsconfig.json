{
  "compilerOptions": {
    "target": "es2016",
    "jsx": "react",
    "module": "esnext",
    "allowJs": true,
    "outDir": "./dist/",
    "declaration": true,
    "declarationMap": true,
    "removeComments": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "moduleResolution": "node"
  }
}
